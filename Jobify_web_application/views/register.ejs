                        aria-label=".form-select-lg example" id="accType">
                        <option selected="selected" value=" "> Please select Account Type</option>
                        <option value="company_role">Company Office</option>
                        <option value="citizen_role">Citizen</option>
                      </select>
                      <div class="invalid-feedback"></div>
                    </div>
                    <!-- Acccount Type -->


                    <!-- birthday -->
                    <div class="form-outline mb-4">
                      <label class="control-label" for="Date Of Birth">
                        Date Of Birth
                      </label>
                      <input id="uDOB" name="uDOB" type="date" placeholder="Date Of Birth"
                        class="form-control form-control-lg border border-secondary">
                      <div class="invalid-feedback"></div>
                    </div>
                    <!-- Name -->
                    <div class="form-outline mb-4">
                      <label class="form-label" for="userName"> Name with Initials</label>
                      <input type="text" id="userName" name="userName"
                        class="form-control form-control-lg border border-secondary" />
                      <div class="invalid-feedback"></div>
                    </div>
                    <!-- email -->
