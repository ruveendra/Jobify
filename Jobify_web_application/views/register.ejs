                    <div class="form-outline mb-4">
                      <label class="form-label" for="nic">NIC</label>
                      <input type="text" id="nic" name="nic"
                        class="form-control form-control-lg border border-secondary" />
                      <div class="invalid-feedback"></div>
                    </div>

                    <!-- Acccount Type -->
                    <!-- industry -->
                    <div class="form-outline mb-4">
